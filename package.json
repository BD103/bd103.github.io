    "feed": "^4.2.2",
    "globals": "^16.0.0",
    "mdsvex": "^0.12.3",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "remark-footnotes": "^2.0.0",
    "svelte": "^5.23.0",
    "svelte-check": "^4.1.5",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
